  },
  "type": "module",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "webpack serve --open --config webpack.config.dev.mjs",
    "build": "webpack --config webpack.config.prod.mjs",
    "test": "mocha test/**/*.mjs",
    "test:coverage": "c8 mocha test/**/*.mjs",
    "lint": "eslint . --ext .mjs,.js",
    "lint:fix": "eslint . --ext .mjs,.js --fix",
    "lint:md": "npx markdownlint-cli2 \"**/*.{md,markdown}\" \"#node_modules/**\" \"#dist/**\" \"#coverage/**\"",
    "lint:css": "stylelint \"**/*.css\" --ignore-path .stylelintignore",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint:all": "npm run lint && npm run lint:md && npm run format",
    "checkout": "npm run test && npm run lint:all",
    "delete": "rm -rf dist/* node_modules package-lock.json",
    "clean": "npm run delete && npm cache clean --force && npm install",
    "upgrade": "npx npm-check-updates -u"
  },
  "dependencies": {
    "astroturf": "^1.2.0",
    "astroturf-loader": "^1.0.0",
    "globalthis": "^1.0.4",
    "postcss-cli": "^11.0.1",
    "safe-stable-stringify": "^2.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/plugin-transform-runtime": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@eslint/js": "^9.29.0",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^10.0.0",
    "browserslist": "^4.25.0",
    "c8": "^10.1.3",
    "chai": "^5.2.0",
    "compression-webpack-plugin": "^11.1.0",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-mocha": "^11.1.0",
    "globals": "^16.2.0",
    "html-webpack-plugin": "^5.6.3",
    "markdownlint": "^0.38.0",
    "markdownlint-cli2": "^0.18.1",
    "mocha": "^11.7.0",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.6.0",
    "style-loader": "^4.0.0",
    "stylelint": "^16.21.0",
    "stylelint-config-alphabetical-order": "^1.0.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "terser-webpack-plugin": "^5.3.14",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-merge": "^6.0.1"
  }
}
